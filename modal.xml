		<Script isActive="yes" isFolder="no">
			<name>mode</name>
			<packageName></packageName>
			<script>if not MODE then MODE = "insert" end

function toggleMode()
  if MODE == "insert" then
    MODE = "normal"
    enableKey('normalmode')
    disableKey('insertMode')
  else
    MODE = "insert"
    enableKey('insertmode')
    disableKey('normalmode')
  end
  resetDoorKeys()
end

function updatePrompt()
  if MODE == "normal" then
  selectString("INS")
  replace("NRM")
  else
  selectString("NRM")
  replace("INS")
  end
end

function openPressed()
  disableKey('movement')
  disableKey('open')
  enableKey('open dirs')
end

function closePressed()
  disableKey('movement')
  disableKey('close')
  enableKey('close dirs')
end

function lockPressed()
  disableKey('movement')
  disableKey('lock')
  enableKey('lock dirs')
end

function unlockPressed()
  disableKey('movement')
  disableKey('unlock')
  enableKey('unlock dirs')
end

function pickPressed()
  disableKey('movement')
  disableKey('pick')
  enableKey('pick dirs')
end

function scoutPressed()
  disableKey('movement')
  disableKey('scout')
  enableKey('scout dirs')
end

function doorbashPressed()
  disableKey('movement')
  disableKey('doorbash')
  enableKey('doorbash dirs')
end

function resetDoorKeys()
  enableKey('movement')
  enableKey('open')
  enableKey('close')
  enableKey('lock')
  enableKey('unlock')
  enableKey('pick')
  enableKey('scout')
  enableKey('doorbash')
  disableKey('open dirs')
  disableKey('close dirs')
  disableKey('lock dirs')
  disableKey('unlock dirs')
  disableKey('pick dirs')
  disableKey('scout dirs')
  disableKey('doorbash dirs')
end</script>
