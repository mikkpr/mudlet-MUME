		<Script isActive="yes" isFolder="no">
			<name>mode</name>
			<packageName></packageName>
			<script>if not MODE then MODE = "insert" end

function toggleMode()
  if MODE == "insert" then
    MODE = "normal"
    enableKey('normalmode')
    disableKey('insertMode')
    tempKey(mudlet.key.Escape, [[ MODE = "insert" ]]) 
  else
    MODE = "insert"
    enableKey('insertmode')
    disableKey('normalmode')
  end
end

function updatePrompt()
  if MODE == "normal" then
  selectString("INS")
  replace("NRM")
  else
  selectString("NRM")
  replace("INS")
  end
end</script>
